{
    "name": "productionapp",
    "resources": {
        "productionapp:index:Deployment": {
            "isComponent": true,
            "inputProperties": {
                "image": {
                    "type": "string",
                    "description": "The image to deploy in your production ready application"
                },
                "port": {
                    "type": "integer",
                    "description": "The port your application listens on in your container image"
                }
            },
            "requiredInputs": [
                "image",
                "port"
            ],
            "properties": {}
        },
        "language": {
            "csharp": {
                "packageReferences": {
                    "Pulumi": "3.*",
                    "Pulumi.Kubernetes": "3.*"
                }
            },
            "go": {
                "generateResourceContainerTypes": true,
                "importBasePath": "github.com/rawkode/pulumi-productionapp/sdk/go/productionapp"
            },
            "nodejs": {
                "dependencies": {
                    "@pulumi/kubernetes": "^3.0.0"
                },
                "devDependencies": {
                    "typescript": "^3.7.0"
                }
            },
            "python": {
                "requires": {
                    "pulumi": ">=3.0.0,<4.0.0",
                    "pulumi-kubernetes": ">=3.0.0,<4.0.0"
                }
            }
        }
    }
}